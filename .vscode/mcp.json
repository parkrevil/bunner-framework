{
  "servers": {
    "angular-cli": {
      "command": "npx",
      "args": ["-y", "@angular/cli", "mcp"]
    },
    "bunner-agentops": {
      "command": "bun",
      "args": ["run", "src/main.ts"],
      "cwd": "bunner-agentops/apps/mcp-server",
      "env": {
        "AGENTOPS_DATA_DIR": "../../../.vscode/.agentops"
      }
    }
  }
}
